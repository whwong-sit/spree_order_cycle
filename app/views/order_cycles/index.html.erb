<h1>Listing order_cycles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start</th>
      <th>End</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_cycles.each do |order_cycle| %>
      <tr>
        <td><%= order_cycle.name %></td>
        <td><%= order_cycle.start %></td>
        <td><%= order_cycle.end %></td>
        <td><%= link_to 'Show', order_cycle %></td>
        <td><%= link_to 'Edit', edit_order_cycle_path(order_cycle) %></td>
        <td><%= link_to 'Destroy', order_cycle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order cycle', new_order_cycle_path %>
